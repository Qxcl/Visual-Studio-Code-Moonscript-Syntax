{
    "name": "moonscript",
    "displayName": "Moonscript Language",
    "description": "Moonscript language syntax highlighting",
    "version": "1.0.6",
    "publisher": "Lynxish-Developer",
    "repository": {
        "type": "git", 
        "url": "https://github.com/owenkimbrell/Visual-Studio-Code-Moonscript-Syntax" 
    },
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Languages",
        "Other"
    ],
    "icon": "CatWithAblunt.jpg",
    "homepage": "https://github.com/owenkimbrell/Visual-Studio-Code-Moonscript-Syntax",
    "author": {
        "name": "Lynxish-Developer"
    },
    "galleryBanner": {
        "theme": "dark"
    },
    "license": "LICENSE",
    "keywords": [
        "moonscript",
        "moon",
        ".moon",
        "Syntax",
        "Language"
    ],
    "contributes": {
        "languages": [
            {
                "id": "moonscript",
                "aliases": [
                    "MoonScript",
                    "moonscript",
                    "moon"
                ],
                "extensions": [
                    ".moon"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "moonscript",
                "scopeName": "source.moonscript",
                "path": "./syntaxes/MoonScriptxx.json",
                "embeddedLanguages": {
                    "scopeName": "lua"
                }
            }
        ],
        "snippets": [
            {
                "language": "moonscript",
                "path": "./snippets/MoonScriptxx-Snippets.json"
            }
        ]
    }
}