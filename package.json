{
    "name": "moonscript",
    "displayName": "Moonscript Language",
    "description": "Moonscript language syntax highlighting",
    "version": "1.0.9",
    "publisher": "Lynxish-Developer",
    "engines": {
        "vscode": "^0.10.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/owenkimbrell/Visual-Studio-Code-Moonscript-Syntax"
    },
    "categories": [
        "Languages",
        "Other",
        "Snippets"
    ],
    "main": "./src/extension.ts",
    "icon": "CatWithAblunt.jpg",
    "homepage": "https://github.com/owenkimbrell/Visual-Studio-Code-Moonscript-Syntax",
    "author": {
        "name": "Lynxish-Developer"
    },
    "galleryBanner": {
        "theme": "dark"
    },
    "license": "./LICENSE",
    "keywords": [
        "moonscript",
        "moon",
        ".moon",
        "Syntax",
        "Language"
    ],
    "contributes": {
        "languages": [
            {
                "id": "moonscript",
                "aliases": [
                    "MoonScript",
                    "moonscript",
                    "moon"
                ],
                "extensions": [
                    ".moon"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "moonscript",
                "scopeName": "source.moonscript",
                "path": "./syntaxes/MoonScriptxx.json",
                "embeddedLanguages": {
                    "scopeName": "scope.lua"
                },
                "injectTo": [
                    "moon"
                ]
            }
        ],
        "snippets": [
            {
                "language": "moonscript",
                "path": "./snippets/MoonScriptxx-Snippets.json"
            }
        ]
    },
    "devDependencies": {
        "@types/mocha": "2.2.32",
        "@types/node": "6.0.40",
        "fs-2": "0.0.1",
        "mocha": "2.3.3",
        "shelljs": "0.7.7",
        "tmp": "0.0.31",
        "ts-stream": "^0.9.1",
        "typescript": "2.4.0-dev.20170606",
        "util": "0.10.3",
        "vscode": "1.2.0"
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "activationEvents": [
        "onLanguage:moonscript"
    ]
}
